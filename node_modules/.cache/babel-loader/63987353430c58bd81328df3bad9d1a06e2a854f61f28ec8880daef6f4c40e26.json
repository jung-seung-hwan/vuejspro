{"ast":null,"code":"import TodoInput from './components/TodoInput.vue';\nimport TodoList from './components/TodoList.vue';\nimport TodoFilter from './components/TodoFilter.vue';\nimport TodoSummary from './components/TodoSummary.vue';\nimport { mapActions } from 'vuex';\nexport default {\n  components: {\n    TodoInput,\n    TodoList,\n    TodoFilter,\n    TodoSummary\n  },\n  data() {\n    return {\n      filterStatus: 'all',\n      // 필터 상태 관리\n      sortByRemainingDays: false // 남은 기간순 정렬 여부\n    };\n  },\n  methods: {\n    ...mapActions(['clearAllTodos']),\n    updateFilter(filterStatus, sortByRemainingDays) {\n      this.filterStatus = filterStatus;\n      this.sortByRemainingDays = sortByRemainingDays;\n    }\n  }\n};","map":{"version":3,"names":["TodoInput","TodoList","TodoFilter","TodoSummary","mapActions","components","data","filterStatus","sortByRemainingDays","methods","updateFilter"],"sources":["C:\\Users\\USER\\Documents\\GitHub\\vuejspro\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" class=\"container\">\r\n    <h1>To-do List</h1>\r\n\r\n    <!-- 할 일 입력 컴포넌트 -->\r\n    <TodoInput />\r\n\r\n    <!-- 필터 및 정렬 -->\r\n    <TodoFilter :filterStatus=\"filterStatus\" :sortByRemainingDays=\"sortByRemainingDays\" @update-filter=\"updateFilter\" />\r\n\r\n    <!-- 완료된 일과 완료되지 않은 일의 수 표시 -->\r\n    <TodoSummary />\r\n\r\n    <!-- 할 일 목록 컴포넌트 -->\r\n    <TodoList :filterStatus=\"filterStatus\" :sortByRemainingDays=\"sortByRemainingDays\" />\r\n\r\n    <!-- 전체 삭제 버튼 -->\r\n    <div class=\"controls\">\r\n      <button class=\"delete-button\" @click=\"clearAllTodos\">전체 삭제</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TodoInput from './components/TodoInput.vue';\r\nimport TodoList from './components/TodoList.vue';\r\nimport TodoFilter from './components/TodoFilter.vue';\r\nimport TodoSummary from './components/TodoSummary.vue';\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n  components: {\r\n    TodoInput,\r\n    TodoList,\r\n    TodoFilter,\r\n    TodoSummary\r\n  },\r\n  data() {\r\n    return {\r\n      filterStatus: 'all',  // 필터 상태 관리\r\n      sortByRemainingDays: false  // 남은 기간순 정렬 여부\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(['clearAllTodos']),\r\n    updateFilter(filterStatus, sortByRemainingDays) {\r\n      this.filterStatus = filterStatus;\r\n      this.sortByRemainingDays = sortByRemainingDays;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 여기에 스타일을 추가하세요 */\r\n</style>\r\n"],"mappings":"AAwBA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,UAAU,EAAE;IACVL,SAAS;IACTC,QAAQ;IACRC,UAAU;IACVC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MAAG;MACtBC,mBAAmB,EAAE,KAAI,CAAG;IAC9B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,eAAe,CAAC,CAAC;IAChCM,YAAYA,CAACH,YAAY,EAAEC,mBAAmB,EAAE;MAC9C,IAAI,CAACD,YAAW,GAAIA,YAAY;MAChC,IAAI,CAACC,mBAAkB,GAAIA,mBAAmB;IAChD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}