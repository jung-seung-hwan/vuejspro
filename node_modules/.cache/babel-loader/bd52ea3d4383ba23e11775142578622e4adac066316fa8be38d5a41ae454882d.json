{"ast":null,"code":"import TodoInput from './components/TodoInput.vue';\nimport TodoList from './components/TodoList.vue';\nimport { mapGetters, mapActions } from 'vuex';\nexport default {\n  components: {\n    TodoInput,\n    TodoList\n  },\n  data() {\n    return {\n      filterStatus: 'all',\n      // 필터 상태 관리\n      sortByRemainingDays: false // 남은 기간순 정렬 여부\n    };\n  },\n  computed: {\n    ...mapGetters(['activeTodosCount', 'completedTodosCount']) // Vuex에서 완료된 일과 완료되지 않은 일의 수 가져오기\n  },\n  methods: {\n    ...mapActions(['clearAllTodos']) // 전체 삭제 Vuex 액션 바인딩\n  }\n};","map":{"version":3,"names":["TodoInput","TodoList","mapGetters","mapActions","components","data","filterStatus","sortByRemainingDays","computed","methods"],"sources":["C:\\Users\\USER\\Documents\\GitHub\\vuejspro\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" class=\"container\">\r\n    <h1 class=\"title\">To-do List</h1>\r\n\r\n    <!-- 할 일 입력 컴포넌트 -->\r\n    <TodoInput />\r\n\r\n    <!-- 필터 드롭다운과 남은 기간순 정렬 버튼 -->\r\n    <div class=\"controls\">\r\n      <label for=\"filter\" class=\"filter-label\">필터:</label>\r\n      <select v-model=\"filterStatus\" id=\"filter\" class=\"filter-select\">\r\n        <option value=\"all\">전체</option>\r\n        <option value=\"active\">미완료</option>\r\n        <option value=\"completed\">완료</option>\r\n      </select>\r\n\r\n      <button class=\"sort-button\" @click=\"sortByRemainingDays = !sortByRemainingDays\">\r\n        {{ sortByRemainingDays ? '남은 기간순 해제' : '남은 기간순 정렬' }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- 완료된 일과 완료되지 않은 일의 수 표시 -->\r\n    <div class=\"summary\">\r\n      <p class=\"summary-text\">완료되지 않은 일: <strong>{{ activeTodosCount }}개</strong></p>\r\n      <p class=\"summary-text\">완료된 일: <strong>{{ completedTodosCount }}개</strong></p>\r\n    </div>\r\n\r\n    <!-- 할 일 목록 컴포넌트 -->\r\n    <TodoList :filterStatus=\"filterStatus\" :sortByRemainingDays=\"sortByRemainingDays\" />\r\n\r\n    <!-- 전체 삭제 버튼 -->\r\n    <div class=\"controls\">\r\n      <button class=\"delete-button\" @click=\"clearAllTodos\">전체 삭제</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TodoInput from './components/TodoInput.vue';\r\nimport TodoList from './components/TodoList.vue';\r\nimport { mapGetters, mapActions } from 'vuex';\r\n\r\nexport default {\r\n  components: {\r\n    TodoInput,\r\n    TodoList\r\n  },\r\n  data() {\r\n    return {\r\n      filterStatus: 'all',  // 필터 상태 관리\r\n      sortByRemainingDays: false  // 남은 기간순 정렬 여부\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['activeTodosCount', 'completedTodosCount'])  // Vuex에서 완료된 일과 완료되지 않은 일의 수 가져오기\r\n  },\r\n  methods: {\r\n    ...mapActions(['clearAllTodos'])  // 전체 삭제 Vuex 액션 바인딩\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  font-family: 'Arial', sans-serif;\r\n  background-color: #f9f9f9;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n  font-size: 2em;\r\n  color: #333;\r\n}\r\n\r\n.controls {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n}\r\n\r\n.filter-label {\r\n  margin-right: 10px;\r\n  font-size: 1.1em;\r\n}\r\n\r\n.filter-select {\r\n  padding: 8px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ddd;\r\n  font-size: 1em;\r\n}\r\n\r\n.sort-button, .delete-button {\r\n  padding: 8px 16px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  background-color: #007BFF;\r\n  color: white;\r\n  font-size: 1em;\r\n  cursor: pointer;\r\n}\r\n\r\n.sort-button:hover, .delete-button:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.summary {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.summary-text {\r\n  font-size: 1.2em;\r\n  color: #333;\r\n}\r\n\r\nstrong {\r\n  color: #007BFF;\r\n}\r\n\r\n.delete-button {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.delete-button:hover {\r\n  background-color: #c82333;\r\n}\r\n</style>\r\n"],"mappings":"AAsCA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAASC,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVJ,SAAS;IACTC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MAAG;MACtBC,mBAAmB,EAAE,KAAI,CAAG;IAC9B,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGN,UAAU,CAAC,CAAC,kBAAkB,EAAE,qBAAqB,CAAC,EAAG;EAC9D,CAAC;EACDO,OAAO,EAAE;IACP,GAAGN,UAAU,CAAC,CAAC,eAAe,CAAC,EAAG;EACpC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}