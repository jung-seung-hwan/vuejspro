{"ast":null,"code":"import TodoInput from './components/TodoInput.vue';\nimport TodoList from './components/TodoList.vue';\nimport { mapActions } from 'vuex';\nexport default {\n  components: {\n    TodoInput,\n    TodoList\n  },\n  data() {\n    return {\n      filterStatus: 'all',\n      // 필터 상태 관리\n      sortByRemainingDays: false // 남은 기간순 정렬 여부\n    };\n  },\n  methods: {\n    ...mapActions(['clearAllTodos']) // 전체 삭제 Vuex 액션 바인딩\n  }\n};","map":{"version":3,"names":["TodoInput","TodoList","mapActions","components","data","filterStatus","sortByRemainingDays","methods"],"sources":["C:\\Users\\USER\\Documents\\GitHub\\vuejspro\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <h1>To-do List</h1>\r\n    <!-- 할 일 입력 컴포넌트 -->\r\n    <TodoInput />\r\n\r\n    <!-- 필터 드롭다운과 남은 기간순 정렬 버튼 -->\r\n    <div>\r\n      <label for=\"filter\">필터:</label>\r\n      <select v-model=\"filterStatus\" id=\"filter\">\r\n        <option value=\"all\">전체</option>\r\n        <option value=\"active\">미완료</option>\r\n        <option value=\"completed\">완료</option>\r\n      </select>\r\n      <button @click=\"sortByRemainingDays = !sortByRemainingDays\">\r\n        {{ sortByRemainingDays ? '남은 기간순 해제' : '남은 기간순 정렬' }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- 전체 삭제 버튼 -->\r\n    <div>\r\n      <button @click=\"clearAllTodos\">전체 삭제</button>\r\n    </div>\r\n\r\n    <!-- 할 일 목록 컴포넌트 -->\r\n    <TodoList :filterStatus=\"filterStatus\" :sortByRemainingDays=\"sortByRemainingDays\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TodoInput from './components/TodoInput.vue';\r\nimport TodoList from './components/TodoList.vue';\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n  components: {\r\n    TodoInput,\r\n    TodoList\r\n  },\r\n  data() {\r\n    return {\r\n      filterStatus: 'all',  // 필터 상태 관리\r\n      sortByRemainingDays: false  // 남은 기간순 정렬 여부\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(['clearAllTodos'])  // 전체 삭제 Vuex 액션 바인딩\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 스타일은 필요에 따라 여기에 추가하세요 */\r\n</style>\r\n"],"mappings":"AA8BA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,UAAU,EAAE;IACVH,SAAS;IACTC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MAAG;MACtBC,mBAAmB,EAAE,KAAI,CAAG;IAC9B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,eAAe,CAAC,EAAG;EACpC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}